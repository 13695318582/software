/* Note:Your choice is C IDE */
#include "stdio.h"
void main()////差最大的两数为该数组的最大值和最小值
{
   int a[10],i,min,max;/////min放最小值，max放最大值
   for(i=0;i<10;i++)
      scanf("%d",&a[i]);
    min=a[0];max=a[0];
   for(i=1;i<10;i++)
      {
      if(min>a[i])
        min=a[i];//把最小值赋值给min
      if(max<a[i])
        max=a[i];//把最大值赋值给max
      }
    printf("%d   %d",min,max);
   
}
